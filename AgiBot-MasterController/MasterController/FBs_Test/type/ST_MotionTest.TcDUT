	m_enterJntTestCar:BOOL;
	
	//start motion flag
	m_startMotionCar:BOOL;
	
	//position incresement mode
	m_jntPosIncModeCar:BOOL;
	
	//car target joint position
	m_targetJntPosCar:Vec4d;
	
	//car target joint velocity
	m_targetJntVelCar:Vec4d;
	
	//target joint position incresement
	m_targetJntPosIncCar:Vec4d;
	
	//target joint current
	m_targetJntCurrentCar:Vec4d;
	
	//enter car joint current test
	m_carJntCurTest:BOOL;
	
	//output the car joint position
	m_carJntPos:Vec4d;
	
	//output the car joint position
	m_carJntCurrent:Vec4d;
	
	//cover command position-car
	m_coverCmdPosCar:BOOL;
