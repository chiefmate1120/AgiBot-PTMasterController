	//target joint position incresement
	m_targetJntPosInc:ARRAY[1..g_mArmNum] OF Vec7d;
	
	//target joint current
	m_targetJntCurrent:ARRAY[1..g_mArmNum] OF Vec7d;
	
	//target joint current
	m_curJntCurrent:ARRAY[1..g_mArmNum] OF Vec7d;
	
	//joint enable
	m_jntEnable:ARRAY[1..g_mArmNum] OF Vec7i;	
	
	
	
	//*********************car**************************//
